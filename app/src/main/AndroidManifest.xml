<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.prasanth.movieapplication">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.TextActivity" />
        <activity android:name=".activities.FindDoryMovieActivity" />
        <activity android:name=".activities.JurassicWorldActivity" />
        <activity android:name=".activities.DoctorStrangeActivity" />
        <activity android:name=".activities.RingsMovieActivity"></activity>
    </application>

</manifest>